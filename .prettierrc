{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": false,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^next",
    "^@?\\w",
    "^\\.\\.(?!/?$)",
    "^\\./(?=.*/)(?!/?$)",
    "^\\./?$",
    "^.+\\.s?css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
